{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": ["warn", { "extensions": [".js", ".jsx"] }],
        "import/prefer-default-export": "off",
        "react/destructuring-assignment": "off",
        "arrow-body-style": ["warn", "as-needed"],
        "no-unused-vars": ["warn"],
        "react/prop-types": "warn"
    }
}
